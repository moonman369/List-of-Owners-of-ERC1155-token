    "update": "npx hardhat run --network localhost scripts/list-of-owners/update-db.js",
    "event": "npx hardhat run --network localhost scripts/list-of-owners/event-listener.js",
    "create-ownerlist": "npx hardhat run --network localhost scripts/list-of-owners/create-ownerlist.js",
