    "transfer-1155": "npx hardhat run --network localhost scripts/transfer-1155.js",
    "display-ownerlist": "npx hardhat run --network localhost scripts/list-of-owners/display-owner-list.js",
    "create-tokenlist": "npx hardhat run --network localhost scripts/list-of-nfts/create-tokenlist.js",
    "update-tokenlist": "npx hardhat run --network localhost scripts/list-of-nfts/update-tokenlist.js",
    "mint-721": "npx hardhat run --network localhost scripts/mint-721.js",
    "display-tokenlist": "npx hardhat run --network localhost scripts/list-of-nfts/display-token-list.js",
    "provider-event": "npx hardhat run --network localhost scripts/list-of-nfts/provider-event-listener.js",
    "delete": "npx hardhat run --network localhost scripts/delete-records.js"
  },
